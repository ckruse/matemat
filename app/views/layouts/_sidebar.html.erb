<% subnav = capture do %>
  <% if logged_in? %>
      <% begin %>
      <%= render 'subnav' %><% @has_subnav = true %>
      <% rescue ActionView::MissingTemplate
        @has_subnav = false
      end %>
  <% end %>
<% end %>
<% if @has_subnav %>
<div class="span2">
  <%= subnav %>
</div>
<% end %>
